<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.text.Font?>


<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="com.winpoint.batchScheduler.fxmlsControllers.ExistBatchPopUpWindowController">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="700.0">
         <bottom>
            <HBox prefHeight="100.0" prefWidth="200.0" style="-fx-background-color: navy blue;" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets bottom="25.0" left="25.0" right="25.0" />
               </BorderPane.margin>
               <children>
                  <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
                  <Button fx:id="closeButton" mnemonicParsing="false" onAction="#validateCloseButton" prefHeight="25.0" prefWidth="100.0" text="Close" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets top="25.0" />
                     </HBox.margin>
                  </Button>
                  <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
         </bottom>
         <top>
            <HBox prefHeight="100.0" prefWidth="200.0" style="-fx-background-color: navy blue;" BorderPane.alignment="CENTER">
               <padding>
                  <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
               </padding>
               <BorderPane.margin>
                  <Insets left="25.0" right="25.0" top="25.0" />
               </BorderPane.margin>
               <children>
                  <Label text="Batch Exist" textFill="WHITE">
                     <font>
                        <Font size="36.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
         </top>
         <center>
            <Label text="Sorry! New batch for this criteria cannot be launched." BorderPane.alignment="CENTER">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
